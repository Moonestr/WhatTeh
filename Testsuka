local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/jensonhirst/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "ğŸ† SpawnScript Vexadezz || Ps99 ğŸ†", HidePremium = false, SaveConfig = true, ConfigFolder = "Ps99Lib"})

local Tab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://6768917255",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "ğŸ‡ Main ğŸ‡"
})

OrionLib:MakeNotification({
	Name = "Vexadez",
	Content = "Script made by @Vexadez yt",
	Image = "rbxassetid://4483345998",
	Time = 5
})

Tab:AddToggle({
	Name = "Spawn pet",
	Default = false,
	Callback = function(Value)
		local Pets = require(game:GetService("ReplicatedStorage").Library.Directory.Pets)

local fromPet = "Jetpack Tiger"
    local toPet = "Huge Gorgon"
    
    if Pets[fromPet] and Pets[toPet] then
        for i, v in pairs(Pets[fromPet]) do
            Pets[fromPet][i] = nil
        end
        for i, v in pairs(Pets[toPet]) do
            Pets[fromPet][i] = v
        end
    end
	end    
})

Tab:AddTextbox({
	Name = "From pet",
	Default = "Select a pet",
	TextDisappear = true,
	Callback = function(Value)
		print(Value)
	end	  
})


Tab:AddTextbox({
	Name = "To pet",
	Default = "Select a pet",
	TextDisappear = true,
	Callback = function(Value)
		print(Value)
	end	  
})
